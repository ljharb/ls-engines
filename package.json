{
	"name": "ls-engines",
	"version": "0.3.2",
	"description": "Determine if your dependency graph's stated \"engines\" criteria is met.",
	"bin": "./bin/ls-engines",
	"exports": {},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"tests-only": "tape test",
		"test": "npm run tests-only",
		"posttest": "npx aud",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ljharb/ls-engines.git"
	},
	"keywords": [
		"engines",
		"npm",
		"node",
		"node_modules"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ljharb/ls-engines/issues"
	},
	"homepage": "https://github.com/ljharb/ls-engines#readme",
	"engines": {
		"node": ">=10",
		"npm": ">=6"
	},
	"dependencies": {
		"@npmcli/arborist": "0.0.0-pre.6",
		"chalk": "^3.0.0",
		"fast_array_intersect": "^1.1.0",
		"get-json": "^1.0.1",
		"json-file-plus": "^3.3.1",
		"pacote": "^10.3.2",
		"promise.allsettled": "^1.0.2",
		"semver": "^7.1.2",
		"yargs": "^15.1.0"
	},
	"devDependencies": {
		"@ljharb/eslint-config": "^16.0.0",
		"auto-changelog": "^1.16.2",
		"eslint": "^6.8.0",
		"tape": "^5.0.0-next.4"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false
	}
}
