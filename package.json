{
	"name": "ls-engines",
	"version": "0.9.3",
	"description": "Determine if your dependency graph's stated \"engines\" criteria is met.",
	"bin": {
		"ls-engines": "bin/ls-engines"
	},
	"exports": {
		"./package.json": "./package.json"
	},
	"scripts": {
		"prepack": "npmignore --auto --commentLines=autogenerated",
		"prepublish": "not-in-publish || npm run prepublishOnly",
		"prepublishOnly": "safe-publish-latest",
		"lint": "eslint . bin/*",
		"pretest": "npm run lint",
		"tests-only": "NODE_NO_WARNINGS=1 NODE_OPTIONS='--require=./test/mocks' nyc tape 'test/*.js'",
		"test": "npm run tests-only",
		"posttest": "npx npm@\">= 10.2\" audit --production",
		"test:update-mocks": "curl -L https://nodejs.org/dist/index.json -o test/mocks/node-versions.json",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ljharb/ls-engines.git"
	},
	"keywords": [
		"engines",
		"npm",
		"node",
		"node_modules"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ljharb/ls-engines/issues"
	},
	"homepage": "https://github.com/ljharb/ls-engines#readme",
	"engines": {
		"node": ">= 19 || ^18 || ^16.13 || ^14.18",
		"npm": ">=8"
	},
	"dependencies": {
		"@npmcli/arborist": "^6.5.1",
		"array.prototype.some": "^1.1.6",
		"array.prototype.tosorted": "^1.1.4",
		"colors": "=1.4.0",
		"fast_array_intersect": "^1.1.0",
		"get-dep-tree": "^2.0.0",
		"get-json": "^1.1.0",
		"json-file-plus": "^3.3.2",
		"lockfile-info": "^1.1.0",
		"object.fromentries": "^2.0.8",
		"object.groupby": "^1.0.3",
		"object.values": "^1.2.1",
		"pacote": "^15.2.0",
		"promise.allsettled": "^1.0.7",
		"semver": "^7.7.3",
		"table": "^6.9.0",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@ljharb/eslint-config": "^21.4.0",
		"auto-changelog": "^2.5.0",
		"encoding": "^0.1.13",
		"es-value-fixtures": "^1.7.1",
		"eslint": "^8.57.1",
		"npmignore": "^0.3.2",
		"nyc": "^10.3.2",
		"object-inspect": "^1.13.4",
		"safe-publish-latest": "^2.0.0",
		"tape": "^5.9.0"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false
	},
	"publishConfig": {
		"ignore": [
			"test/fixtures/**",
			"test/mocks/**",
			".github/workflows"
		]
	}
}
